{
  "name": "pnpm-turbo-monorepo-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "release-it"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.45.0",
    "eslint-config-icebreaker": "^1.2.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "release-it": "^16.1.3",
    "ts-node": "^10.9.1",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.6.7"
}