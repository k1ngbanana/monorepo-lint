{
  "name": "@icebreakers/monorepo",
  "type": "module",
  "version": "0.0.0",
  "description": "my monorepo config copyer",
  "author": "",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts"
  },
  "bin": "src/bin.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublish": "tsx scripts/prepublish.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": "bin/copy.js"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "pathe": "^1.1.2"
  }
