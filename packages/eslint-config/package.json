{
  "name": "@banana020/eslint-config",
  "type": "module",
  "version": "0.0.0",
  "description": "tsup(esbuild) build package template",
  "author": "banana <k1ng_banana@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k1ng_banana/monorepo-lint.git",
    "directory": "packages/bar"
  },
  "bugs": {
    "url": "https://github.com/k1ng_banana/monorepo-lint/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "defu": "^6.1.4",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-mdx": "3.1.5",
    "eslint-plugin-tailwindcss": "3.17.5",
    "eslint-plugin-vuejs-accessibility": "^2.4.1"
  },
  "devDependencies": {
    "@types/eslint-plugin-tailwindcss": "^3.17.0"
  }
}
